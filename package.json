{
  "name": "quantum",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build",
    "build-prod": "ng build -t production -e prod && gulp compress",
    "build-prod-qa": "ng build --prod --configuration=qa   --output-path output --delete-output-path false  && gulp compress",
    "build-prod-dev": "ng build --prod   --output-path output --delete-output-path false  && gulp compress",
    "refreshVSToken": "vsts-npm-auth -config .npmrc",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "build-ngsw": "npm run ngsw-config && npm run ngsw-copy",
    "build:ssr:dev": "npm run build:client-and-server-bundles:dev && npm run webpack:server",
    "build:client-and-server-bundles:dev": "ng build --prod  && ng run quantum:server",
    "sw-prod-webpack": "rimraf ./dist/app/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --color && workbox injectManifest ./workbox-config.js",
    "old-build:ssr": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "ng build --prod --configuration=qa  && ng run quantum:server:qa",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node --trace-deprecation dist/server/main.js",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server -p 8080",
    "build:ssr": "ng build --configuration production --aot --source-map=false && ng run quantum:server:production && gulp compress",
    "build:ssrdev": "ng build --configuration development --aot && ng run quantum:server:development && gulp compress",
    "dev:ssr": "ng run quantum:serve-ssr",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "prerender": "ng run quantum:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.0.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.0.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/pwa": "12.0.0",
    "@angular/router": "~12.2.0",
    "@angular/service-worker": "12.2.16",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@microsoft/signalr": "^5.0.0",
    "@ngrx/effects": "12.2.0",
    "@ngrx/entity": "12.2.0",
    "@ngrx/store": "12.2.0",
    "@ngrx/store-devtools": "12.2.0",
    "@nguniversal/express-engine": "^12.0.0",
    "angular2-recaptcha": "^1.1.0",
    "app-action-icon": "^1.8.9",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "gzipper": "^7.2.0",
    "ng-recaptcha": "9.0.0",
    "ngx-image-cropper": "^2.0.0",
    "ngx-sharebuttons": "^9.0.0",
    "oidc-client": "^1.11.3",
    "oidc-client-ts": "^2.0.6",
    "rxjs": "6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.0.0",
    "@types/express": "4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
